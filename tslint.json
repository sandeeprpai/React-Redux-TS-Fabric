{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": false,
    "no-console":false,
    "jsx-no-lambda": false,
    "jsx-wrap-multiline": false,
    "jsx-self-close": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
